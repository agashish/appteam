{{#each noteList}}
<div class="row">
    <div class="col s12 m12 my_team_list">
    <div class="card-panel grey lighten-5 z-depth-1">
        <div class="row valign-wrapper">
        <div class="col s1 my_team_image">
            <img src="/images/avatar.jpg" alt="" class="circle responsive-img"> <!-- notice the "circle" class -->
        </div>
        <div class="col s11">
            <div class="my_team_name">{{../user.firstname}}<span>Jan 11, 2016</span></div>
            <span class="black-text">
                {{this.notes}}
            </span>
        </div>
        </div>
    </div>
    </div> 
</div>
<div class="clear">&nbsp;</div>
{{/each}}